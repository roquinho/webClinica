<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>PacientesServlet</servlet-name>
        <servlet-class>Controller.PacientesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MedicosServlet</servlet-name>
        <servlet-class>Controller.MedicosServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConsultasServlet</servlet-name>
        <servlet-class>Controller.ConsultasServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExamesServlets</servlet-name>
        <servlet-class>Controller.ExamesServlets</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuariosServlets</servlet-name>
        <servlet-class>Controller.UsuariosServlets</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PacientesServlet</servlet-name>
        <url-pattern>/PacientesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MedicosServlet</servlet-name>
        <url-pattern>/MedicosServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConsultasServlet</servlet-name>
        <url-pattern>/ConsultasServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExamesServlets</servlet-name>
        <url-pattern>/ExamesServlets</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuariosServlets</servlet-name>
        <url-pattern>/UsuariosServlets</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
